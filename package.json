{
    "name": "drissy",
    "version": "2.0.0",
    "description": "``` Install with: composer create-project hendrikeng/drissy PROJEKTNAME -s RC",
    "keywords": [
        "Example",
        "Keywords"
    ],
    "homepage": "https://github.com/hendrikeng/drissy#readme",
    "bugs": {
        "url": "https://github.com/hendrikeng/drissy/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Hendrik Werner",
        "email": "hendrik@wewereyoung.de",
        "url": "https://wewereyoung.de"
    },
    "browser": "/web/index.php",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hendrikeng/drissy.git"
    },
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js --progress --hide-modules"
    },
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR"
        ],
        "legacyBrowsers": [
            "> 1%",
            "last 2 versions",
            "Firefox ESR"
        ],
        "modernBrowsers": [
            "last 2 Chrome versions",
            "not Chrome < 60",
            "last 2 Safari versions",
            "not Safari < 10.1",
            "last 2 iOS versions",
            "not iOS < 10.3",
            "last 2 Firefox versions",
            "not Firefox < 54",
            "last 2 Edge versions",
            "not Edge < 15"
        ]
    },
    "babel": {
        "extends": "./build-config/babel.config.js"
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "./build-config/eslint.config.js"
        ]
    },
    "eslintIgnore": [
        "/node_modules/**",
        "/modules/**",
        "/storage/**",
        "/vendor/**",
        "/src/fonts/**",
        "/src/vendor/**",
        "/src/js/inline/**",
        "/templates/_inlinejs/**",
        "/web/**",
        "/build-config/tailwind.config.js"
    ],
    "stylelint": {
        "extends": [
            "./build-config/stylelint.config.js"
        ],
        "ignoreFiles": [
            "node_modules/**/*",
            "modules/**/*",
            "storage/**/*",
            "vendor/**/*",
            "src/fonts/**/*",
            "src/vendor/**/*",
            "web/**/*",
            "css-gridish/**/*"
        ]
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 80,
        "proseWrap": "preserve",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "overrides": [
            {
                "files": "*.json",
                "options": {
                    "printWidth": 200
                }
            }
        ]
    },
    "dependencies": {
        "@babel/core": "^7.1.2",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/register": "^7.0.0",
        "@glidejs/glide": "^3.2.3",
        "axios": "^0.18.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^0.1.19",
        "cookieconsent": "^3.1.0",
        "create-symlink-webpack-plugin": "^1.0.0",
        "critical": "^1.3.4",
        "critical-css-webpack-plugin": "^0.2.0",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.7",
        "eslint": "^5.8.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "eslint-plugin-vue": "^4.7.1",
        "fg-loadcss": "^2.0.1",
        "file-loader": "^2.0.0",
        "fontfaceobserver": "^2.0.13",
        "glob-all": "^3.1.0",
        "gsap": "^2.0.2",
        "html-webpack-plugin": "^3.2.0",
        "ignore-loader": "^0.1.2",
        "imagemin": "^6.0.0",
        "imagemin-gifsicle": "^5.2.0",
        "imagemin-mozjpeg": "^7.0.0",
        "imagemin-optipng": "^5.2.1",
        "imagemin-svgo": "^7.0.0",
        "imagemin-webp": "^4.1.0",
        "imagemin-webp-webpack-plugin": "^1.0.2",
        "img-loader": "^3.0.1",
        "lazysizes": "^4.1.4",
        "mini-css-extract-plugin": "^0.4.4",
        "moment": "^2.22.2",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "photoswipe": "^4.1.2",
        "postcss": "^7.0.5",
        "postcss-extend": "^1.0.5",
        "postcss-fixes": "^2.0.1",
        "postcss-hexrgba": "^1.0.1",
        "postcss-import": "^12.0.1",
