{
  "name": "drissy",
  "version": "1.1.5-beta",
  "description": "``` Install with: composer create-project hendrikeng/drissy PROJEKTNAME -s RC",
  "main": "webpack.config.js",
  "author": "Hendrikeng <hendrik@wewereyoung.de>",
  "license": "MIT",
  "scripts": {
    "babel:inlinejs": "npx babel ./src/js/inline/ --out-dir ./src/js/inline/",
    "build": "yarn clean && yarn --silent webpack --profile --json > compilation-stats.json --env.env=prod",
    "build:all": "yarn build --env.plugins=favicon --env.plugins=workbox",
    "build:analyze": "yarn build:all --env.plugins=bundleanalyzer",
    "build:bundleanalyzer": "yarn build --env.plugins=bundleanalyzer",
    "build:compression": "yarn build --env.plugins=compression",
    "build:favicon": "yarn build --env.plugins=favicon",
    "build:purge": "yarn build --env.plugins=purge",
    "build:workbox": "yarn build --env.plugins=workbox",
    "clean": "yarn clean:assets && yarn clean:inlinejs && yarn clean:appcache && yarn clean:sw",
    "clean:assets": "rm -rf ./web/assets/*",
    "clean:appcache": "rm -rf ./web/appcache/*",
    "clean:sw": "rm -rf ./web/*.js",
    "clean:inlinejs": "rm -rf ./templates/_inlinejs/*",
    "critical": "node ./build-utils/critical-all.js",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "yarn webpack-dev-server --env.env=dev",
    "dev:init": "yarn clean && yarn grid && yarn babel:inlinejs && yarn uglify:inlinejs && yarn init:tailwind",
    "dev:bundleanalyzer": "yarn dev --env.plugins=bundleanalyzer",
    "dev:workbox": "yarn dev --env.plugins=workbox",
    "dev:hardsource": "yarn dev --env.plugins=hardsource",
    "dev:all": "yarn dev --env.plugins=hardsource --env.plugins=bundleanalyzer",
    "grid": "css-gridish",
    "init:tailwind": "rm -rf ./build-config/tailwind.config.js && ./node_modules/.bin/tailwind init ./build-config/tailwind.config.js",
    "postinstall": "npm cache verify",
    "uglify:inlinejs": "node ./build-utils/uglify.js",
    "webpack": "node ./node_modules/webpack/bin/webpack.js ",
    "webpack-defaults": "webpack-defaults",
    "webpack-dev-server": "webpack-dev-server"
  },
  "stylelint": {
    "extends": [
      "./build-config/stylelint.config.js"
    ],
    "ignoreFiles": [
      "node_modules/**/*",
      "modules/**/*",
      "storage/**/*",
      "vendor/**/*",
      "src/fonts/**/*",
      "src/vendor/**/*",
      "web/**/*",
      "css-gridish/**/*"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./build-config/eslint.config.js"
    ]
  },
  "eslintIgnore": [
    "node_modules/**/*",
    "modules/**/*",
    "storage/**/*",
    "vendor/**/*",
    "src/fonts/**/*",
    "src/vendor/**/*",
    "src/js/inline/**/*",
    "templates/_inlinejs/**/*",
    "web/**/*",
    "tailwind.config.js"
  ],
  "babel": {
    "extends": "./build-config/babel.config.js"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/engereauswahl/drissy.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "compression-webpack-plugin": "^1.1.11",
    "critical": "^1.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-vue": "^4.5.0",
    "favicons-webpack-plugin": "git+https://github.com/hendrikeng/favicons-webpack-plugin.git#pull_theme_start_manifest",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "portfinder": "^1.0.13",
    "postcss": "^6.0.21",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-preset-env": "^5.1.0",
    "purgecss-webpack-plugin": "^1.0.2",
    "sane": "^2.5.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "tailwindcss": "^0.5.2",
    "uglify-js": "^3.3.22",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-eslint-parser": "^2.0.3",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-stylish": "^0.1.8",
    "webpackbar": "^2.6.1",
    "workbox": "^0.0.0",
    "workbox-webpack-plugin": "^3.1.0",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "css-gridish": "^2.0.0",
    "fg-loadcss": "^2.0.1",
    "fontfaceobserver": "^2.0.13",
    "lazysizes": "^4.0.2",
    "typeface-lato": "^0.0.54",
    "vue": "^2.5.16"
  },
  "bugs": {
    "url": "https://github.com/engereauswahl/drissy/issues"
  },
  "homepage": "https://github.com/engereauswahl/drissy#readme"
}
