{
    "name": "hendrikeng/drissy",
    "description": "WeWereYoung - Craft 3 CMS - Webpack 4 - Tailwind - Vue - scaffolding project",
    "version": "2.0.2",
    "keywords": ["craft", "cms", "craftcms", "project", "webpack", "tailwind", "vue"],
    "license": "MIT",
    "homepage": "https://craftcms.com/",
    "type": "project",
    "support": {
        "email": "hendrik@wewereyoung.de",
        "issues": "https://github.com/hendrikeng/drissy/issues",
        "source": "https://github.com/hendrikeng/drissy",
        "docs": "https://github.com/hendrikeng/drissy"
    },
    "minimum-stability": "stable",
    "require": {
        "aelvan/imager": "^2.0",
        "craftcms/cms": "^3.0.0",
        "craftcms/redactor": "^2.0.0.1",
        "doublesecretagency/craft-cpcss": "^2.1.0",
        "mmikkel/child-me": "^1.0",
        "mmikkel/cp-field-inspect": "^1.0",
        "nystudio107/craft-cookies": "^1.1",
        "nystudio107/craft-eagerbeaver": "^1.0.0",
        "nystudio107/craft-minify": "^1.2.5",
        "nystudio107/craft-routemap": "^1.0.0",
        "nystudio107/craft-scripts": "^1.2.4",
        "nystudio107/craft-seomatic": "^3.0",
        "nystudio107/craft-twigpack": "^1.1",
        "nystudio107/craft-typogrify": "^1.1.1",
        "ostark/craft-async-queue": "^1.3.0",
        "sebastianlenz/linkfield": "1.0.8",
        "topshelfcraft/environment-label": "^3.1",
        "verbb/expanded-singles": "^1.0",
        "verbb/image-resizer": "2.0.3",
        "vlucas/phpdotenv": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php craft setup/welcome"
        ],
        "post-update-cmd": [
            "@php craft clear-caches/all"
        ],
        "post-install-cmd": [
            "@php craft clear-caches/all"
        ]
      }
    }
